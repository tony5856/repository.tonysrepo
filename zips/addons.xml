<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.Conflux" name="Conflux Addon" version="2.2.9" provider-name=" - ">
    <requires>
        <import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.universalscrapers" version="1.0.0" />
        <import addon="script.module.python.koding.aio" version="0.9.6.6" />
        <import addon="script.module.beautifulsoup" version="3.2.1" />
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.addon.common" version="2.0.0" />
        <import addon="script.module.xmltodict" version="0.9.0" />
        <!--<import addon="script.module.web-pdb" version="1.0"/>-->
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="[login|startup]" />
    <extension point="xbmc.addon.metadata">
        <summary lang="en">All in one addon</summary>
        <description lang="en">Movies, TV Shows, Documentaries, Cartoons, ect...</description>
        <platform>all</platform>
        <website></website>
        <video_guide></video_guide>
    </extension>
</addon>

<addon id="repository.tonysrepo" name="Tonys Repo" version="1.0.3" provider-name="TonyH">
	<extension point="xbmc.addon.repository" name="Tonys Repo">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Repository with my addons and builds</summary>
		<description lang="en">Contains my builds, my wizard, and my addons</description>
		<genre>devtools,documentary,movies,space,sports,tvshows,wizard,uk,usa</genre>
	</extension>
</addon>
</addons>
