<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.xmlerator" name="XMLerator" version="5.0.2" provider-name="TonyH">
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Generate xml's from online lists</summary>
		<description lang="en">Generate xml's from user made lists using list numbers from tmdb, imdb, and trakt</description>
		<platform>all</platform>
		<forum></forum>
		<website></website>
		<news></news>
		<genre>uk,usa</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>

<addon id="repository.tonysrepo" name="Tonys Repo" version="1.0.4" provider-name="TonyH">
	<extension point="xbmc.addon.repository" name="Tonys Repo">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/teamuniversal/scrapers/master/_modules4all/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/teamuniversal/scrapers/master/_modules4all/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/teamuniversal/scrapers/master/_modules4all/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/tony5856/repository.tonysrepo/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Repository with my addons and builds</summary>
		<description lang="en">Contains my builds, my wizard, and my addons</description>
		<genre>devtools,documentary,movies,space,sports,tvshows,wizard,uk,usa</genre>
	</extension>
</addon>
</addons>
